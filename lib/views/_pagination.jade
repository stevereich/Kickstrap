ul.pagination.center-block(style="display: table")
	li(ks-visible="currentPage != 0")
		a(href="", ng-click="deincrement()") &laquo;
	li(ng-repeat="genericItem in genericItems | orderByPriority | limitTo:numPages()", class="{{ currentPage == $index ? 'active': ''}}", ng-click="setCurrentPage($index)")
		a(href="") {{$index+1}}
	li(ks-visible="currentPage < (numPages() - 1)")
		a(href="", ng-click="increment()") &raquo;	